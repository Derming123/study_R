@article{ ISI:000327938200007,
Author = {Darling, Tom and Mahfuz, Istiak and White, Stefan J. and Cheng, Wei},
Title = {{No TAP63 promoter mutation is detected in bladder exstrophy-epispadias
   complex patients}},
Journal = {{JOURNAL OF PEDIATRIC SURGERY}},
Year = {{2013}},
Volume = {{48}},
Number = {{12}},
Pages = {{2393-2400}},
Month = {{DEC}},
Note = {{46th Annual Meeting of the Pacific-Association-of-Pediatric-Surgeons
