@article{ ISI:000314697200005,
Author = {Zaneveld, Jacques and Wang Feng and Wang Xia and Chen Rui},
Title = {{Dawn of ocular gene therapy: implications for molecular diagnosis in
   retinal disease}},
Journal = {{SCIENCE CHINA-LIFE SCIENCES}},
