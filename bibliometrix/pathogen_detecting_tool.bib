@article{ ISI:000370963300005,
Author = {Dudekulay, Dawood B. and Panda, Amaresh C. and Grammatikakis, Ioannis
   and De, Supriyo and Abdelmohsen, Kotb and Gorospe, Myriam},
Title = {{CircInteractome: A web tool for exploring circular RNAs and their
   interacting proteins and microRNAs}},
