@article{ ISI:000347916900012,
Author = {Feng, Zhixing and Li, Jing and Zhang, Jing-Ren and Zhang, Xuegong},
Title = {{qDNAmod: a statistical model-based tool to reveal intercellular
   heterogeneity of DNA modification from SMRT sequencing data}},
Journal = {{NUCLEIC ACIDS RESEARCH}},
