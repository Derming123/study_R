@article{ ISI:000296234800002,
Author = {Pedersen, Anders Elm and Stryhn, Anette and Justesen, Sune and Harndahl,
   Mikkel and Rasmussen, Susanne and Donskov, Frede and Claesson, Mogens H.
   and Pedersen, Johannes W. and Wandall, Hans H. and Svane, Inge Marie and
   Buus, Soren},
Title = {{Wildtype p53-specific Antibody and T-Cell Responses in Cancer Patients}},
Journal = {{JOURNAL OF IMMUNOTHERAPY}},
Year = {{2011}},
Volume = {{34}},
